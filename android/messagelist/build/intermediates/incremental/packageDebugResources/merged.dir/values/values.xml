    <dimen name="aurora_size_date_text">10sp</dimen>
    <dimen name="aurora_size_event_text">10sp</dimen>
    <dimen name="aurora_size_receive_text">14sp</dimen>
    <dimen name="aurora_size_send_text">14sp</dimen>
    <dimen name="aurora_width_conv_avatar">50dp</dimen>
    <dimen name="aurora_width_msg_avatar">42dp</dimen>
    <dimen name="link_radio">6dp</dimen>
    <dimen name="message_padding">48dp</dimen>
    <dimen name="photo_width">166dp</dimen>
    <dimen name="red_packet_comments_size">16sp</dimen>
