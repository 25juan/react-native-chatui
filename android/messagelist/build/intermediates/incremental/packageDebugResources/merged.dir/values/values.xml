    <dimen name="aurora_avatar_padding">6dp</dimen>
    <dimen name="aurora_height_conv_avatar">50dp</dimen>
    <dimen name="aurora_height_msg_avatar">42dp</dimen>
    <dimen name="aurora_message_padding_top">4dp</dimen>
    <dimen name="aurora_padding_date_text">5dp</dimen>
    <dimen name="aurora_padding_event_text">5dp</dimen>
